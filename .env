# TurboNet Environment Configuration
# Target IP for the shredder (use 127.0.0.1 for local tests)
TURBONET_TARGET_IP=127.0.0.1

# Ports used for the three data lanes
LANE1_PORT=8001
LANE2_PORT=8002
LANE3_PORT=8003

# Variable Shredder Weights (e.g. 10/45/45)
SHRED_W0=10
SHRED_W1=45
SHRED_W2=45

# Deep-Sea Fragmentation Block Size (bytes)
BLOCK_SIZE=5242880

# LEVEL 7: AUTO-PILOT (DYNAMIC RTT)
TURBONET_DYNAMIC=true

# LEVEL 8: NEURAL STRATEGIST (DEEPSEEK R1 8B)
TURBONET_NEURAL=true
OLLAMA_MODEL=deepseek-r1:8b
